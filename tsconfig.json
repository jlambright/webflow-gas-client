{
  "compilerOptions": {
    "baseUrl": "src",
    "experimentalDecorators": true,
    "forceConsistentCasingInFileNames": true,
    "lib": ["esnext"],
    "moduleResolution": "node",
    "module": "esnext",
    "target": "esnext",
    "sourceMap": true,
    "strict": true,
    "strictNullChecks": true,
    "esModuleInterop": true,
    "outDir": "build",
    "types": ["google-apps-script"]
  },
  "exclude": [
    "node_modules"
  ],
  "include": [
    "src/*"
  ]
}